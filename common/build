#! /bin/bash

# This script copies all the necessary components in the right folder

cp execute_implementation ../m1stack
cp execute_implementation ../m1interpreter
cp execute_implementation ../m3orderedmap

cp execute_test ../m1stacktests
cp execute_test ../m1interpretertests
cp execute_test ../m3tests
cp execute_test ../m4_plagiarism_tests
cp execute_test ../m5_compressor_tests
cp execute_test ../m6_kruskal_tests

preexams=(
    "preexam_bfs"
    "preexam_dfs"
    "preexam_merge_sort"
)

for folder in "${preexams[@]}"
do
    cp execute_preexam "../${folder}"
    cp RunTests.java "../${folder}/student"
done
